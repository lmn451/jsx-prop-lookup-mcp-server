{
  "name": "jsx-prop-lookup-mcp-server",
  "version": "4.0.0",
  "description": "MCP server for analyzing JSX prop usage using AST (ReScript + TypeScript)",
  "main": "dist/index.js",
  "type": "module",
  "keywords": [
    "mcp",
    "jsx",
    "react",
    "ast",
    "props",
    "analysis",
    "typescript",
    "rescript"
  ],
  "author": "Rovo Dev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lmn451/jsx-prop-lookup-mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/lmn451/jsx-prop-lookup-mcp-server/issues"
  },
  "homepage": "https://github.com/lmn451/jsx-prop-lookup-mcp-server#readme",
  "scripts": {
    "build:js": "bun build ./src/index.ts ./src/cli.ts --outdir ./dist --target node",
    "build:types": "tsc --emitDeclarationOnly",
    "copy-test-fixtures": "cp -r src/__tests__/fixtures dist/__tests__/fixtures",
    "build": "bun run build:js && bun run build:types && bun run copy-test-fixtures",
    "build:rescript": "rescript build",
    "build:rescript:clean": "rescript clean && rescript build",
    "dev:rescript": "rescript build -w",
    "typecheck": "tsc --noEmit",
    "typecheck:rescript": "rescript build -dry-run",
    "start": "bun dist/index.js",
    "start:rescript": "node src/mcp/Index.mjs",
    "dev": "bun src/index.ts",
    "dev:rescript": "rescript build -w",
    "test": "vitest",
    "test:rescript": "rescript build && bun test",
    "prepublishOnly": "bun run build"
  },
  "bin": {
    "jsx-prop-lookup-mcp-server": "dist/index.js",
    "jsx-analyzer": "dist/cli.js"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "dependencies": {
    "@babel/parser": "^7.28.4",
    "@babel/traverse": "^7.28.4",
    "@babel/types": "^7.28.4",
    "@modelcontextprotocol/sdk": "^1.20.0",
    "zod": "^3.23.8",
    "glob": "^11.0.3",
    "rescript-bun": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "rescript": "^12.0.0"
  },
  "packageManager": "bun@1.2.2"
}