{
  "name": "jsx-prop-lookup-mcp-server",
  "version": "3.1.0-beta.0",
  "description": "MCP server for analyzing JSX prop usage using AST",
  "main": "dist/index.js",
  "type": "module",
  "keywords": [
    "mcp",
    "jsx",
    "react",
    "ast",
    "props",
    "analysis",
    "typescript"
  ],
  "author": "Rovo Dev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lmn451/jsx-prop-lookup-mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/lmn451/jsx-prop-lookup-mcp-server/issues"
  },
  "homepage": "https://github.com/lmn451/jsx-prop-lookup-mcp-server#readme",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\" --ext .js,.ts,.tsx",
    "lint:fix": "eslint \"src/**/*.{js,ts,tsx}\" --ext .js,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,md}\"",
    "test": "node --test tests/**/*.test.js",
    "test:watch": "node --test --watch tests/**/*.test.js",
    "test:coverage": "node --test --experimental-test-coverage tests/**/*.test.js"
  },
  "bin": {
    "jsx-prop-lookup-mcp-server": "dist/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "dependencies": {
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@babel/types": "^7.23.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "glob": "^10.3.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "5.3.3",
    "eslint": "^8.50.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.0"
  }
}
